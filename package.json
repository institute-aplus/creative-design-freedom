        "mini-css-extract-plugin": "^2.6.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "swc-loader": "^0.2.3",
        "terser-webpack-plugin": "^5.3.6",
