        "@types/jest": "^29.2.0",
        "@types/node": "^18.11.7",
        "@types/offscreencanvas": "^2019.7.0",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.41.0",
        "babel-loader": "^9.1.0",
        "browserslist": "^4.21.4",
